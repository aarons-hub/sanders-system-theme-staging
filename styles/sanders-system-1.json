{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"title": "Sanders System bg rounded",
	"slug": "sanders-system-bg-rounded",
	"blockTypes": ["core/columns", "core/group", "core/column"],
	"styles": {
		"border": {
			"radius": "20px"
		},
		"color": {
			"background": "var(--wp--preset--color--background-lighter)",
			"text": "var(--wp--preset--color--text-primary)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary-blue)"
				}
			}
		},
		"spacing": {
			"padding": {
				"top": "var(--wp--preset--spacing--50);",
				"right": "var(--wp--preset--spacing--50)",
				"bottom": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)"
			}
		}
	}
}
